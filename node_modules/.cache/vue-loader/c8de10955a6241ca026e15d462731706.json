            </div>\n            </div>\n            <div class=\"row\" >\n                <div class=\"col-lg-3 col-md-6\" v-for=\"item in toparticles\"> \n                    <div class=\"single-post-wrap\">\n                        <div class=\"thumb\">\n                            <img src=\"@/assets/img/post/11.png\" alt=\"img\">\n                            <a class=\"tag top-right tag-sky\" href=\"#\">分类{{item.cate}}</a>\n                        </div>\n                        <div class=\"post-details\">\n                            <div class=\"meta\">\n                                <div class=\"date\">\n                                    <i class=\"fa fa-clock-o\"></i>\n                                    {{item.created_at}}\n                                </div>\n                            </div>  \n                            <h6 style=\"height:3em;\" ><a :href=\"'#/detail/' + item.id \" target=\"_blank\" >{{item.title}}</a></h6>\n                        </div>\n                    </div>\n                </div> \n            </div>\n        </div>\n    </div> \n    \n    <!-- news-area Start -->\n    <div class=\"pd-top-70 pd-bottom-30\">\n        <div class=\"container\">\n            <div class=\"row\">\n                <div class=\"col-lg-8\">\n                    <div class=\"section-title\">\n                        <h4 class=\"title\">Stories</h4>\n                        <span class=\"line\"></span>\n                    </div>\n                    <div class=\"media-post-wrap-2 media\" v-for=\"item in articles\">\n                        <div class=\"thumb\">\n                            <img src=\"@/assets/img/post/18.png\" alt=\"img\">\n                        </div>\n                        <div class=\"media-body\">\n                            <a class=\"tag tag-red\" href=\"#\">{{item.cate}}</a>\n                            <div class=\"meta d-block mt-3\">\n                                <div class=\"date\">\n                                    <i class=\"fa fa-clock-o\"></i>\n                                    {{item.created_at}}\n                                </div>\n                            </div>\n                            <h6><a :href=\"'#/detail/' + item.id \" target=\"_blank\" >{{item.title}}</a></h6>\n                            <p >{{item.content}}</p>\n                        </div>\n                    </div> \n                    <div class=\"btn-wrap mt-5 mb-5 text-center\">\n                        <a class=\"btn btn-main\" href=\"#\">Load More</a>\n                    </div>\n                </div>\n                <div class=\"col-lg-4\">\n                    <div class=\"section-title\">\n                        <h4 class=\"title\">Categories</h4>\n                        <span class=\"line\"></span>\n                    </div>\n                    <ul class=\"widget widget-categories\">\n                        <li v-for=\"item in cateObjList\">\n                            <div class=\"thumb\"><img src=\"@/assets/img/category/1.png\" alt=\"img\"></div>\n                            <a :href=\"'#/category/' + item.id \" target=\"_blank\" >{{item.title}} <span class=\"bg-sky\">{{item.id}}</span></a>\n                        </li> \n                    </ul>\n                    <a class=\"mb-5 d-block\" href=\"#\">\n                        <img class=\"w-100\" src=\"assets/img/ad/2.png\" alt=\"img\">\n                    </a>\n                </div>\n            </div>\n        </div>\n    </div>\n    <!-- news-area End -->  \n     <Footer/>\n</div> \n</template> \n<script>\nimport Header from \"../header/header\"\nimport Footer from \"../footer/footer\"\nimport { cateList,articleList } from \"@/api/article.js\";   \nexport default {\n  name: 'Category',\n  async created() { \n    console.log(this.$route.params)\n\n    var cate_id = this.$route.params.cate_id\n\n     // 页面一打开就去加载列表。\n    let cateRes = await cateList(); \n    this.cateObjList = cateRes.data.data; \n    let articleRes = await articleList({\"page\":1}); \n    this.toparticles = articleRes.data.data.slice(0,4) \n    let articleRes2 = await articleList({\"page\":1,\"cid\":cate_id}); \n    this.articles = articleRes2.data.data\n    //文章内容处理\n    for(var j = 0 ; j < this.articles.length; j++) {\n        var tmp = this.articles[j].content.split(\" \")\n        var newcont = tmp.slice(0,30).join(' ') + \"...\" \n        this.articles[j].content = newcont  \n    } \n    \n  },\n  data() {\n    return {\n      toparticles:{},\n      articles:{},\n      cateObjList:[]\n    };\n  },\n  components: {\n    Header,Footer\n  }\n\n}\n\n \n\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped> \n \n</style>\n"]}]}