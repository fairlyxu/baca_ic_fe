IAAI,CAACK,QAAN,CAAb;AACD;AACF;AALI;AAAA;AAAA;AAAA;AAAA;AAMN;;;;;;AAEH,eAAe,IAAIP,QAAJ,EAAf","sourcesContent":["class Interval {\n  constructor() {\n    this.tasks = [];\n  }\n  addTask(task, time = 1000) {\n    this.tasks.push({ task, time });\n  }\n  run(that) {\n    if (this.tasks.length < 1) {\n      return;\n    }\n    for (const task of this.tasks) {\n      if (task.interval) {\n        clearInterval(task.interval);\n      }\n      task.task();\n      var timesRun = 0\n      task.interval = setInterval(() => {\n        task.task();\n        timesRun += 1;\n        if(timesRun === 20){\n          clearInterval(task.interval);\n        }\n      }, task.time);\n    }\n  }\n  stop() {\n    for (const task of this.tasks) {\n      if (task.interval) {\n        clearInterval(task.interval);\n      }\n    }\n  }\n}\nexport default new Interval();"]}]}