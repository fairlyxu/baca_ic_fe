AAE,OAAV,GAAqBH,MAAM,CAACI,GAAvC;AAEJ,SAAOV,OAAO,CAAC;AACXG,IAAAA,GAAG,EAAEF,IAAI,GAAG,oBAAP,GAA8BQ,QADxB;AAEXL,IAAAA,MAAM,EAAE;AAFG,GAAD,CAAd;AAIH,C,CAKD;;AACA,OAAO,SAASQ,aAAT,CAAuBN,MAAvB,EAA+B;AAClC;AACA;AACA,SAAON,OAAO,CAAC;AACXG,IAAAA,GAAG,EAAEF,IAAI,GAAG,0BAAP,aAAsCK,MAAM,CAACO,MAA7C,CADM;AAEXT,IAAAA,MAAM,EAAE;AAFG,GAAD,CAAd;AAIH,C,CAMD;;AACA,OAAO,SAASU,aAAT,CAAuBC,IAAvB,EAA6B;AAChC,SAAOf,OAAO,CAAC;AACXG,IAAAA,GAAG,EAAE,2BADM;AAEXC,IAAAA,MAAM,EAAE,MAFG;AAGXW,IAAAA,IAAI,EAAJA;AAHW,GAAD,CAAd;AAKH,C,CAED;;AACA,OAAO,SAASC,WAAT,CAAqBD,IAArB,EAA2B;AAC9B,SAAOf,OAAO,CAAC;AACXG,IAAAA,GAAG,EAAE,2BADM;AAEXC,IAAAA,MAAM,EAAE,MAFG;AAGXW,IAAAA,IAAI,EAAJA;AAHW,GAAD,CAAd;AAKH,C,CAGD;;AACA,OAAO,SAASE,WAAT,GAAuB;AAC1B,SAAOjB,OAAO,CAAC;AACXG,IAAAA,GAAG,EAAE,oBADM;AAEXC,IAAAA,MAAM,EAAE;AAFG,GAAD,CAAd;AAIH,C,CAED;;AACA,OAAO,SAASc,WAAT,CAAqBH,IAArB,EAA2B;AAC9B,SAAOf,OAAO,CAAC;AACXG,IAAAA,GAAG,EAAE,2BADM;AAEXC,IAAAA,MAAM,EAAE,MAFG;AAGXW,IAAAA,IAAI,EAAJA;AAHW,GAAD,CAAd;AAKH,C,CACD;;AACA,OAAO,SAASI,cAAT,CAAwBJ,IAAxB,EAA8B;AACjC,SAAOf,OAAO,CAAC;AACXG,IAAAA,GAAG,sCAA+BY,IAAI,CAACF,MAApC,CADQ;AAEXT,IAAAA,MAAM,EAAE;AAFG,GAAD,CAAd;AAIH,C,CAED;;AACA,OAAO,SAASgB,eAAT,CAAyBL,IAAzB,EAA+B;AAClC,SAAOf,OAAO,CAAC;AACXG,IAAAA,GAAG,EAAE,4BADM;AAEXC,IAAAA,MAAM,EAAE,MAFG;AAGXW,IAAAA,IAAI,EAAJA;AAHW,GAAD,CAAd;AAKH,C,CAED;;AACA,OAAO,SAASM,aAAT,CAAuBN,IAAvB,EAA6B;AAChC,SAAOf,OAAO,CAAC;AACXG,IAAAA,GAAG,uCAAgCY,IAAI,CAACF,MAArC,CADQ;AAEXT,IAAAA,MAAM,EAAE;AAFG,GAAD,CAAd;AAIH,C,CAED;;AACA,OAAO,SAASkB,UAAT,CAAoBP,IAApB,EAA0B;AAC7B,SAAOf,OAAO,CAAC;AACXG,IAAAA,GAAG,EAAE,+BADM;AAEXC,IAAAA,MAAM,EAAE,MAFG;AAGXW,IAAAA,IAAI,EAAJA;AAHW,GAAD,CAAd;AAKH,C,CAED;;AACA,OAAO,SAASQ,YAAT,CAAsBR,IAAtB,EAA4B;AAC/B,SAAOf,OAAO,CAAC;AACXG,IAAAA,GAAG,0CAAmCY,IAAI,CAACF,MAAxC,CADQ;AAEXT,IAAAA,MAAM,EAAE;AAFG,GAAD,CAAd;AAIH","sourcesContent":["import request from '@/utils/request.js'\nconst host=\"http://ec2-54-178-107-66.ap-northeast-1.compute.amazonaws.com/\"\n\n\n// 1，获取频道列表\nexport function cateList() {\n    return request({\n        url: host + \"/api/article/category\",\n        method: \"get\" \n    })\n}\n\n//获取文章列表\nexport function articleList(params) {  \n    let page = 1\n    if (params.hasOwnProperty(\"page\") &&   (params.page != null)){\n        page = params.page\n    }  \n    var parmstmp = \"page=\" + page \n    if (params.hasOwnProperty(\"cid\") && (params.cid == null) && (params.cid !== undefined))\n        parmstmp = parmstmp +'&cid=' +  params.cid \n    \n    return request({\n        url: host + \"/api/article/list?\" + parmstmp, \n        method: \"get\" \n    })\n}\n\n\n\n\n// 获取文章详情\nexport function articleDetail(params) {\n    // 因为这个接口只要一个路径参数\n    // 所以没必要在后面在写params了，如果后面写了调用接口可能会报错。\n    return request({  \n        url: host + \"/api/article/detail?aid=\" +`${params.art_id}`,  \n        method: \"get\"\n    })\n}\n\n\n \n \n\n//  举报文章\nexport function articleReport(data) {\n    return request({\n        url: \"/app/v1_0/article/reports\",\n        method: \"post\",\n        data\n    })\n}\n\n//  拉黑用户\nexport function authorBlack(data) {\n    return request({\n        url: \"/app/v1_0/user/blacklists\",\n        method: \"post\",\n        data\n    })\n}\n\n\n// 获取文章评论或评论回复接口\nexport function getComments() {\n    return request({\n        url: \"/app/v1_0/comments\",\n        method: \"get\"\n    })\n}\n\n//点赞文章\nexport function articleLike(data) {\n    return request({\n        url: \"/app/v1_0/article/likings\",\n        method: \"post\",\n        data\n    })\n}\n// 取消文章点赞\nexport function articleDisLike(data) {\n    return request({\n        url: `/app/v1_0/article/likings/${data.art_id}`,\n        method: \"DELETE\",\n    })\n}\n\n// 对文章不喜欢\nexport function articleBuXiHuan(data) {\n    return request({\n        url: \"/app/v1_0/article/dislikes\",\n        method: \"post\",\n        data\n    })\n}\n\n//  对文章取消不喜欢\nexport function articleXiHuan(data) {\n    return request({\n        url: `/app/v1_0/article/dislikes/${data.art_id}`,\n        method: \"DELETE\",\n    })\n}\n\n// 对文章收藏\nexport function artCollect(data) {\n    return request({\n        url: \"/app/v1_0/article/collections\",\n        method: \"post\",\n        data\n    })\n}\n\n// 对文章取消收藏\nexport function artUnCollect(data) {\n    return request({\n        url: `/app/v1_0/article/collections/${data.art_id}`,\n        method: \"DELETE\",\n    })\n}\n\n"]}]}