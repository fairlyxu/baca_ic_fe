IAAAA,GAAG,EAAEF,IAAI,GAAG,uBAAP,GAAiCQ,QAD3B;AAEXL,IAAAA,MAAM,EAAE;AAFG,GAAD,CAAd;AAIH,C,CAED;;AACA,OAAO,SAASQ,WAAT,CAAqBN,MAArB,EAA6B;AAChCO,EAAAA,OAAO,CAACC,GAAR,CAAYR,MAAZ;AACA,MAAIC,IAAI,GAAG,CAAX;;AACA,MAAID,MAAM,CAACE,cAAP,CAAsB,MAAtB,KAAoCF,MAAM,CAACC,IAAP,IAAe,IAAvD,EAA6D;AACzDA,IAAAA,IAAI,GAAGD,MAAM,CAACC,IAAd;AACH;;AACD,MAAIE,QAAQ,GAAG,UAAUF,IAAzB;AAEA,MAAID,MAAM,CAACE,cAAP,CAAsB,KAAtB,KAAiCF,MAAM,CAACI,GAAP,KAAe,IAAhD,IAA0DJ,MAAM,CAACI,GAAP,KAAeC,SAA7E,EACIF,QAAQ,GAAGA,QAAQ,GAAE,OAAV,GAAqBH,MAAM,CAACI,GAAvC;AAEJ,SAAOV,OAAO,CAAC;AACXG,IAAAA,GAAG,EAAEF,IAAI,GAAG,oBAAP,GAA8BQ,QADxB;AAEXL,IAAAA,MAAM,EAAE;AAFG,GAAD,CAAd;AAIH,C,CAKD;;AACA,OAAO,SAASW,aAAT,CAAuBT,MAAvB,EAA+B;AAClC,SAAON,OAAO,CAAC;AACXG,IAAAA,GAAG,EAAEF,IAAI,GAAG,0BAAP,aAAsCK,MAAM,CAACU,MAA7C,CADM;AAEXZ,IAAAA,MAAM,EAAE;AAFG,GAAD,CAAd;AAIH,C,CAID;;AACA,OAAO,SAASa,WAAT,CAAqBX,MAArB,EAA6B;AAChC,SAAON,OAAO,CAAC;AACXG,IAAAA,GAAG,EAAEF,IAAI,GAAI,wBAAR,GAAmCK,MAAM,CAACY,GAA1C,GAAgD,SAD1C;AAEXd,IAAAA,MAAM,EAAE;AAFG,GAAD,CAAd;AAIH,C,CAGD;;AACA,OAAO,SAASe,cAAT,CAAwBb,MAAxB,EAAgC;AACnC,SAAON,OAAO,CAAC;AACXG,IAAAA,GAAG,EAAEF,IAAI,GAAI,wBAAR,GAAmCK,MAAM,CAACY,GAA1C,GAAgD,oBAD1C;AAEXd,IAAAA,MAAM,EAAE;AAFG,GAAD,CAAd;AAIH","sourcesContent":["import request from '@/utils/request.js'\nconst host=\"http://ec2-54-178-107-66.ap-northeast-1.compute.amazonaws.com/\"\n\n\n// 1，获取频道列表\nexport function cateList() {\n    return request({\n        url: host + \"/api/article/category\",\n        method: \"get\" \n    })\n}\n\n\n//\n//获取文章列表\nexport function articleTopVoteList(params) {  \n    let page = 1\n    if (params.hasOwnProperty(\"page\") &&   (params.page != null)){\n        page = params.page\n    }  \n    var parmstmp = \"page=\" + page \n\n    if (params.hasOwnProperty(\"cid\") && (params.cid !== null) && (params.cid !== undefined))\n        parmstmp = parmstmp +'&cid=' +  params.cid \n  \n    \n    return request({\n        url: host + \"/api/article/topVote?\" + parmstmp, \n        method: \"get\" \n    })\n}\n\n//获取文章列表\nexport function articleList(params) { \n    console.log(params)\n    let page = 1\n    if (params.hasOwnProperty(\"page\") &&   (params.page != null)){\n        page = params.page\n    }  \n    var parmstmp = \"page=\" + page \n\n    if (params.hasOwnProperty(\"cid\") && (params.cid !== null) && (params.cid !== undefined))\n        parmstmp = parmstmp +'&cid=' +  params.cid \n    \n    return request({\n        url: host + \"/api/article/list?\" + parmstmp, \n        method: \"get\" \n    })\n}\n\n\n\n\n// 获取文章详情\nexport function articleDetail(params) { \n    return request({  \n        url: host + \"/api/article/detail?aid=\" +`${params.art_id}`,  \n        method: \"get\"\n    })\n}\n \n \n\n//点赞文章\nexport function articleLike(params) {\n    return request({\n        url: host +  \"/api/article/like?aid=\" + params.aid + \"&like=1\",\n        method: \"get\" \n    })\n}\n \n\n// 对文章不喜欢 \nexport function articleDisLike(params) {\n    return request({\n        url: host +  \"/api/article/like?aid=\" + params.aid + \"&type=unlike&add=1\",\n        method: \"get\" \n    })\n}\n\n \n  "]}]}