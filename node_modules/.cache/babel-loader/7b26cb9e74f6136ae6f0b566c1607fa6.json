AASU,WAAT,CAAqBR,MAArB,EAA6B;AAChC,SAAON,OAAO,CAAC;AACXG,IAAAA,GAAG,EAAEF,IAAI,GAAI,wBAAR,GAAmCK,MAAM,CAACS,GAA1C,GAAgD,SAD1C;AAEXX,IAAAA,MAAM,EAAE;AAFG,GAAD,CAAd;AAIH,C,CAGD;;AACA,OAAO,SAASY,cAAT,CAAwBV,MAAxB,EAAgC;AACnC,SAAON,OAAO,CAAC;AACXG,IAAAA,GAAG,EAAEF,IAAI,GAAI,wBAAR,GAAmCK,MAAM,CAACS,GAA1C,GAAgD,WAD1C;AAEXX,IAAAA,MAAM,EAAE;AAFG,GAAD,CAAd;AAIH","sourcesContent":["import request from '@/utils/request.js'\nconst host=\"http://ec2-54-178-107-66.ap-northeast-1.compute.amazonaws.com/\"\n\n\n// 1，获取频道列表\nexport function cateList() {\n    return request({\n        url: host + \"/api/article/category\",\n        method: \"get\" \n    })\n}\n\n//获取文章列表\nexport function articleList(params) {  \n    let page = 1\n    if (params.hasOwnProperty(\"page\") &&   (params.page != null)){\n        page = params.page\n    }  \n    var parmstmp = \"page=\" + page \n    if (params.hasOwnProperty(\"cid\") && (params.cid == null) && (params.cid !== undefined))\n        parmstmp = parmstmp +'&cid=' +  params.cid \n    \n    return request({\n        url: host + \"/api/article/list?\" + parmstmp, \n        method: \"get\" \n    })\n}\n\n\n\n\n// 获取文章详情\nexport function articleDetail(params) { \n    return request({  \n        url: host + \"/api/article/detail?aid=\" +`${params.art_id}`,  \n        method: \"get\"\n    })\n}\n \n \n\n//点赞文章\nexport function articleLike(params) {\n    return request({\n        url: host +  \"/api/article/like?aid=\" + params.aid + \"&like=1\",\n        method: \"get\" \n    })\n}\n \n\n// 对文章不喜欢\nexport function articleDisLike(params) {\n    return request({\n        url: host +  \"/api/article/like?aid=\" + params.aid + \"&unlike=1\",\n        method: \"get\" \n    })\n}\n\n \n  "]}]}